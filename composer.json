{
    "name": "aammui/laravel-taggable",
    "description": "Assign Category and Tags to a model",
    "license": "MIT",
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Aammui\\LaravelTaggableServiceProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aammui\\LaravelTaggableServiceProvider\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aammui\\LaravelTaggable\\LaravelTaggableServiceProvider"
            ]
        }
    }
}